<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/color_000000</item>
        <item name="colorPrimaryDark">@color/color_000000</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!-- 牌照框弹出动画 -->
    <style name="picture_picker_animstyle">
        <item name="android:windowEnterAnimation">@anim/photo_dialog_in_anim</item>
        <item name="android:windowExitAnimation">@anim/photo_dialog_out_anim</item>
    </style>
    <style name="titlestyle">
        <item name="android:textSize">12sp</item>
    </style>
    <style name="toolBar">
        <item name="colorControlNormal">#ffffff</item>
        <item name="android:homeAsUpIndicator">@mipmap/cha</item>
        <item name="android:titleTextStyle">@style/titlestyle</item>
    </style>

</resources>
